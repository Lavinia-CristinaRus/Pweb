<!DOCTYPE html>
<head>
  <title>completed tasks</title>
  <link rel="stylesheet" href="style.css">
</head>
<body>
  <div class="row">
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Task</th>
        <th scope="col">Time</th>
     
      </tr>
    </thead>
    <tbody>
      <% if(data.length){
     
      for(var i = 0; i< data.length; i++) {%>  
      <tr>
        <th scope="row"><%= (i+1) %></th>
        <td><%= data[i].denumire%></td>
        <td><%= data[i].timp%></td>
        <td>                                            
       </td>
      </tr>
      <% }
               
       }else{ %>
           <tr>
              <td colspan="3">No available tasks in your department</td>
           </tr>
        <% } %>    
      
    </tbody>
    </table>
  </div>
</body>
</html>