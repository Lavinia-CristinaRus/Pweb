<!DOCTYPE html>
<head>
  <title>colleagues</title>
  <link rel="stylesheet" href="style.css">
  <link rel="table" href="table.css">
</head>
<body>
  <div class="row">
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
     
      </tr>
    </thead>
    <tbody>
      <% if(data.length){
     
      for(var i = 0; i< data.length; i++) { %>  
      <tr>
        <th scope="row"><%= (i+1) %></th>
        <td><%= data[i].prenume%></td>
        <td><%= data[i].nume%></td>
        <td>                                            
       </td>
      </tr>
      <% }
               
       }else{ %>
           <tr>
              <td colspan="3">No user</td>
           </tr>
        <% } %>    
      
    </tbody>
    </table>
  </div>
</body>
</html>